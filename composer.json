{
    "name": "georgetour/drupal-composer-docker-workflow",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "George Tourtsinakis",
            "email": "george@besmartbesimple.com",
            "role": "Maintainer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "dinbror/blazy",
                "version": "1.8.2",
                "type": "drupal-library",
                "extra": {
                  "installer-name": "blazy"
                },
                "source": {
                    "type": "git",
                    "url": "https://github.com/dinbror/blazy",
                    "reference": "1.8.2"
                }
             }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/autogrow",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor.autogrow"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/autogrow/releases/autogrow_4.8.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/codemirror",
                "version": "v1.17.5",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor.codemirror"
                },
                "dist": {
                    "url": "https://github.com/w8tcha/CKEditor-CodeMirror-Plugin/releases/download/v1.17.5/CKEditor-CodeMirror-Plugin.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/image",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor.image"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/image/releases/image_4.8.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/link",
                "version": "4.8.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor.link"
                },
                "dist": {
                    "url": "https://download.ckeditor.com/link/releases/link_4.8.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "codemirror/codemirror",
                "version": "5.36.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "codemirror"
                },
                "dist": {
                    "url": "https://github.com/components/codemirror/archive/5.36.0.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        }
    ],
    "require": {
        "ckeditor/autogrow": "4.8.0",
        "ckeditor/codemirror": "v1.17.5",
        "ckeditor/fakeobjects": "4.8.0",
        "ckeditor/image": "4.8.0",
        "ckeditor/link": "4.8.0",
        "codemirror/codemirror": "5.36.0",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "desandro/imagesloaded": "*",
        "desandro/masonry": "*",
        "dimsemenov/photoswipe": "*",
        "dinbror/blazy": "1.8.2",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.23",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/blazy_photoswipe": "*",
        "drupal/block_content_permissions": "^1.6",
        "drupal/colorbox": "^1.4",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.3",
        "drupal/console": "^1.0.2",
        "drupal/core": "~8.6",
        "drupal/default_content": "8.x-1.x-dev",
        "drupal/devel": "^1.2",
        "drupal/dropzonejs": "^2.0",
        "drupal/drush_language": "^1.0@RC",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/empty_page": "^2.0",
        "drupal/entity_browser": "^2.0",
        "drupal/entity_embed": "^1.0",
        "drupal/eu_cookie_compliance": "^1.0",
        "drupal/field_group": "^1.0",
        "drupal/file_browser": "^1.1",
        "drupal/google_analytics": "^2.2",
        "drupal/image_effects": "^2.1",
        "drupal/image_style_quality": "^1.3",
        "drupal/inline_entity_form": "^1.0",
        "drupal/lang_dropdown": "^2.0",
        "drupal/link_attributes": "^1.2",
        "drupal/linkit": "^4.3",
        "drupal/mailsystem": "^4.1",
        "drupal/media_entity_browser": "^2.0",
        "drupal/metatag": "^1.5",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.2",
        "drupal/photoswipe": "^1.0",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.2",
        "drupal/responsive_menu": "^2.6",
        "drupal/search_api": "^1.8",
        "drupal/shield": "^1.2",
        "drupal/simple_sitemap": "^2.12",
        "drupal/slick": "^1.0",
        "drupal/slick_views": "^1.0@RC",
        "drupal/token": "^1.1",
        "drupal/twig_tweak": "^2.0",
        "drupal/video_embed_field": "^2.0",
        "drupal/webform": "^5.0@RC",
        "drush/drush": "^9.0.0",
        "enyo/dropzone": "*",
        "frdh/mmenu": "*",
        "jquery/geocomplete": "1.7.0",
        "jquery/inputmask": "3.3.11",
        "jquery/intl-tel-input": "12.1.12",
        "jquery/rateit": "1.1.1",
        "jquery/select2": "4.0.5",
        "jquery/timepicker": "1.11.13",
        "jquery/toggles": "4.0.0",
        "jquery/word-and-character-counter": "2.5.1",
        "kenwheeler/slick": "*",
        "progress-tracker/progress-tracker": "1.4.0",
        "signature_pad/signature_pad": "2.3.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "public_html/core": ["type:drupal-core"],
            "public_html/libraries/{$name}": ["type:drupal-library"],
            "public_html/modules/contrib/{$name}": ["type:drupal-module"],
            "public_html/profiles/contrib/{$name}": ["type:drupal-profile"],
            "public_html/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
